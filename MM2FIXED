strafeButton.Text = "Enable Strafe"
strafeButton.Size = UDim2.new(0, 160, 0, 50)
strafeButton.Position = UDim2.new(0.05, 0, 0.85, 0)
strafeButton.BackgroundColor3 = Color3.fromRGB(0, 200, 255)
