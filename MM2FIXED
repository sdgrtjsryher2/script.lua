local killAuraEnabled = false
local hitbox = Instance.new("Part")


hitbox.Size = Vector3.new(10, 10, 10)
hitbox.Anchored = true
hitbox.CanCollide = false
hitbox.Transparency = 1
hitbox.BrickColor = BrickColor.new("Bright red")
hitbox.Parent = workspace
hitbox.Material = Enum.Material.ForceField


HitboxVisible.MouseButton1Click:Connect(function()
	local visible = false
	if visible == false then
		hitbox.Transparency = 0
		visible = true
	elseif hitbox == true then
		hitbox.Transparency = 1
		visible = false
	end
end)

local function updateHitboxPosition(player)
	if killAuraEnabled and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
		hitbox.Position = player.Character.HumanoidRootPart.Position
	else
		hitbox.Position = Vector3.new(0, -50, 0) 
	end
end


local function activateItem(player)
	if not player or not player.Character then return end

	local character = player.Character
	local backpack = player:FindFirstChild("Backpack")
	if not backpack then return end

	local knifeInBackpack = backpack:FindFirstChild("Knife")
	local knifeInCharacter = character:FindFirstChild("Knife")
	local currentItemInCharacter = character:FindFirstChildOfClass("Tool")

	if knifeInCharacter then
		knifeInCharacter:Activate()

	elseif knifeInBackpack then
		if currentItemInCharacter then
			currentItemInCharacter.Parent = backpack 
		end
		knifeInBackpack.Parent = character 
		knifeInBackpack:Activate()
		knifeInCharacter = character:FindFirstChild("Knife")
		if knifeInCharacter then
			knifeInCharacter.Parent = backpack 
		end
	end
end

hitbox.Touched:Connect(function(hit)
	local player = game.Players:GetPlayerFromCharacter(hit.Parent)
	if player and player ~= game.Players.LocalPlayer then
		activateItem(player)
	end
end)


game:GetService("RunService").RenderStepped:Connect(function()
	updateHitboxPosition(game.Players.LocalPlayer)
end)


